<h1>Login </h1>

<%= form_for(:user) do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %><br>



    <%= f.label :password %>
    <%= f.password_field :password %><br>



    <%= f.submit "Login" %>
<% end  %>

       
            <%= button_to 'Login With GOOGLE',"/auth/google_oauth2", class: 'nav-link' %>
        