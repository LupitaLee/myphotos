

<section class="container-fluid bg5">

<div class="add-box">



<%= render partial: 'errors/errors', locals: {model: @photo} %>

<%= form_for(@photo)do |f| %>
  

<%= f.label :title %>
<%= f.text_field :title %>



<%= f.label :image %>
<%= f.file_field :image %>


<%= f.submit 'Submit Photo' %>
<% end %>


</div>

</section>